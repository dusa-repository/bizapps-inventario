			<div id="botoneraF49041" />
			<separator />
			<groupbox class="azul" id="gpxRegistro" width="auto"
				title="" open="false">
				<caption label="Nuevo" style="font-weight:bold"
					tooltiptext="Presione para Registrar un Nuevo Chofer" />
				<separator />
				<grid width="100%" style="border:0px">
					<columns visible="false">
						<column label="" width="18%" />
						<column label="" width="35%" />
						<column label="" width="12%" />
						<column label="" width="35%" />
					</columns>
					<rows>
						<row>
							<hbox>
								<label class="etiqueta"
									value="N° empleado:" />
								<space />
								<label value="*"
									style="font-weight:bold;color:red">
								</label>
							</hbox>
							<textbox id="txtSTFNF49041" hflex="1"
								tooltiptext="Seleccione del Catalogo el Codigo del Empleado (ABAN8 de F0101)" />
							<button id="btnBuscarF0101" label="Buscar"
								class="btn" tooltiptext="Catalogo de Codigos de Empleados (F0101)"
								image="/public/imagenes/botones/buscar.png" />
							<label id="lblF0101" />
						</row>
						<row>
							<hbox>
								<label class="etiqueta"
									value="ID vehículo:" />
								<space />
								<label value="*"
									style="font-weight:bold;color:red">
								</label>
							</hbox>
							<textbox id="txtVEHIF49041" hflex="1"
								maxlength="12" tooltiptext="Seleccione del Catalogo el Codigo del Vehiculo (VSSTFN de F4930)"/>
							<button id="btnBuscarF4930" label="Buscar"
								class="btn" tooltiptext="Catalogo de Codigos de Vehiculos (F4930)"
								image="/public/imagenes/botones/buscar.png" />
							<label id="lblF4930" />
						</row>
						<row>
							<hbox>
								<label class="etiqueta"
									value="Fecha referencia:" />
								<space />
								<label value="*"
									style="font-weight:bold;color:red"/>
							</hbox>

							<datebox readonly="true" width="100%"
								id="dtbEFTJF49041" format="dd-MM-yyyy"
								tooltiptext="Ingrese la Fecha de referencia (VSEFTJ)" />
							<hbox />
							<hbox />
						</row>
						<row>
							<cell colspan="3">
								<div id="divBuscadorSHFTF49041"
									hflex="1" />
							</cell>
							<hbox />
						</row>
						<row>
							<label class="etiqueta" value="Depósito:" />

							<textbox id="txtMCU49041" hflex="1"
								maxlength="12" tooltiptext="Seleccione del Catalogo el Codigo del Deposito (MCMCU de F0006)"/>
							<button id="btnBuscarF0006" label="Buscar"
								class="btn" tooltiptext="Catalogo de Codigos de Depositos (F0006)"
								image="/public/imagenes/botones/buscar.png" />
							<label id="lblF0006" />
						</row>
					</rows>
				</grid>
			</groupbox>
			<separator />
			<groupbox class="azul" id="gpxDatos" width="auto"
				title="">
				<caption label="Registros" style="font-weight:bold"
					tooltiptext="Presione para Visualizar los choferes" />
				<div id="divCatalogoF49041"
					tooltiptext="Seleccione el chofer Para Realizar una Accion" />
